    "# Final global E & H fields, reduced to magnitudes, and reversed\n",
    "EVals = [abs(element[0]) for element in Vals]\n",
    "HVals = [abs(element[1]) for element in Vals]\n",
    "\n",
    "EVals.reverse()\n",
    "HVals.reverse()\n",
    "\n",
    "# Plot Details\n",
